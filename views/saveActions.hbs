<div class="row">
    <div class="col s12 ">
        <div class="card blue-grey darken-2 ">
            <h2 class="center-align white-text">Сохраненные списки Actions</h2>
        </div>
    </div>
</div>
<div class="row">
    <div class="col s12 ">
        <div class="card blue-grey darken-2 mzk-actions">
            <div class="card-content white-text">
                <div id="noCoverList" class="card-content white-text no-cover-list">
                    <div class="collection collection-list">
                        {{#if list.length}}
                        {{#each list}}
                        <a href="/save-actions/{{list_id}}/edit" class="collection-item"
                            data-id={{list_id}}>{{list_name}} <span class="right">создано - {{date}}</span></a>
                        {{/each}}
                        {{else}}
                        <p href="#!" class="collection-item teal-text center-align">Записей нет!</p>
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>