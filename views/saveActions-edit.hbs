<div class="row">
    <div class="col s12 ">
        <div class="card blue-grey darken-2 mzk-actions">
            <div class="card-content white-text no-cover-list edit-watch">
                <span class="card-title">
                    <strong> {{list.list_name}}</strong> <button class="btn waves-effect waves-light right tooltipped reAnalitic"
                        data-position="top" data-tooltip="Проверка покрытия по текущему списку"><i
                            class="material-icons">analytics</i> </button>
                </span>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col s12 ">
        <div class="card blue-grey darken-2 mzk-actions">
            <div id="noCoverList" class="card-content white-text no-cover-list edit-watch">
                {{#if list.actions.length}}
                <table class="highlight">
                    <tbody>
                        {{#each list.actions}}
                        <tr>
                            <td><strong>{{action}}</strong></td>
                            <td>{{name}}</td>
                            <td>{{description}}</td>
                            <td>
                                <button class="btn btn-small red darken-1 waves-effect waves-light js-remove"
                                    data-id={{id}}>
                                    <i data-id={{id}} class="material-icons trash js-remove">delete_forever</i>
                                </button>
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
                {{else}}
                <p>Нет actions</p>
                {{/if}}
            </div>
        </div>
    </div>
</div>